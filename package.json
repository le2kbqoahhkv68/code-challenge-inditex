    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "axios": "^0.21.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "react-router-dom": "^5.2.0",
    "sass-resources-loader": "^2.1.1",
